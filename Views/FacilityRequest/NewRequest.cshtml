            {

                <div class="content-wrapper">
                    <section class="content">
                        <div class="container">
                            <table class="table table-striped">
                                <tbody>
                                    <tr>
                                        <td colspan="5">
                                            <div class="well form-horizontal">
                                                <fieldset>
                                                    @if (Model.IsStep1)
